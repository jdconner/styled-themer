{
    "name": "styled-variants",
    "version": "2.1.0",
    "description": "A scalable styled-component theming system that fully leverages JavaScript as a language for styles authoring and theming.",
    "author": "Justin Conner <justindconner2@gmail.com> (https://justinconner.dev/)",
    "homepage": "https://github.com/jdconner/styled-variants/blob/master/README.md",
    "license": "MIT",
    "keywords": [
        "styled-variants",
        "variants",
        "theming",
        "styled-components",
        "theming styled-components",
        "scalable",
        "first class objects"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.esm.js",
    "browser": "dist/index.umd.js",
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "node test/test.js",
        "example": "(cd ./example && npm run start)",
        "pretest": "npm run build",
        "publish": "npm run build && npm publish"
    },
    "dependencies": {
        "lodash.defaultsdeep": "^4.6.1"
    },
    "devDependencies": {
        "rollup": "^1.20.0",
        "rollup-plugin-commonjs": "^10.0.0",
        "rollup-plugin-node-resolve": "^5.1.0",
        "rollup-plugin-terser": "^5.1.2",
        "rollup-plugin-typescript": "^1.0.1",
        "rollup-plugin-uglify": "^6.0.3",
        "tslib": "^1.10.0",
        "typescript": "^2.9.2"
    }
}
